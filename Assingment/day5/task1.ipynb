   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "7847e85c-663e-45b5-836d-371b468e50f7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def numpy_read(file_name):\n",
    "    path = os.path.normpath(file_name)\n",
    "    df = pd.read_csv(path)\n",
    "    return df"
   ]
